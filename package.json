{
  "name": "persona-centric-financial-platform",
  "version": "1.0.0",
  "description": "Persona Centric Financial Platform - AI-powered financial advisory platform",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\"",
    "db:reset": "node server/utils/resetDatabase.js",
    "seed": "node server/utils/seeder.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "sequelize": "^6.35.0",
    "sqlite3": "^5.1.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "financial",
    "ai",
    "advisory",
    "platform",
    "persona-centric"
  ],
  "author": "Persona Centric Team",
  "license": "MIT"
}
